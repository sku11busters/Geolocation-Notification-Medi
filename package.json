{
  "name": "ahj-media",
  "version": "1.0.0",
  "main": "index.js",
  "license": "ISC",
  "test": {
    "testEnvironment": "node",
    "testTimeout": 35000
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint . --fix",
    "test": "jest --testPathPattern=./src/ --passWithNoTests --detectOpenHandles",
    "testcover": "jestjest --testPathPattern=./src/ --passWithNoTests --coverage",
    "doc": "jsdoc -c jsdoc.conf.json",
    "preshow:coverage": "npm run testcover",
    "show:coverage": "live-server testcover/lcov-report",
    "preshow:dist": "npm run build",
    "show:dist": "live-server dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.2",
    "@babel/preset-env": "^7.21.4",
    "appveyor": "^0.4.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.7.2",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.1.5",
    "eslint-plugin-prettier": "^4.2.1",
    "html-loader": "^4.2.0",
    "jest": "^29.3.1",
    "live-server": "^1.2.2",
    "prettier": "2.7.1",
    "resolve-url-loader": "^5.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "chokidar": "3.0.0",
    "chromium": "^3.0.3",
    "core-js": "^3.29.1",
    "html-webpack-plugin": "^5.5.0",
    "ini": "1.3.6",
    "jest-each": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "mini-css-extract-plugin": "^2.7.5",
    "puppeteer": "^19.8.5",
    "puppeteer-core": "^19.8.5",
    "push-dir": "^0.4.1",
    "request": "^2.9.0",
    "typescript": "^5.0.4",
    "uuid": "7.0.0"
  }
}
